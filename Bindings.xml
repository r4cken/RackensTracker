<Bindings>
	<!-- Toggle panel -->
	<!-- See https://us.forums.blizzard.com/en/wow/t/bindingsxml-taint-and-other-issues/1394681/8 for why there is an empty space at the end of the category name-->
	<Binding name="RACKENSTRACKER_TOGGLE" category="BINDING_HEADER_RACKENSTRACKER">
		SlashCmdList["ACECONSOLE_RACKENSTRACKER"]("toggle")
	</Binding>
	<Binding name="RACKENSTRACKER_OPTIONS_OPEN" category="BINDING_HEADER_RACKENSTRACKER">
		SlashCmdList["ACECONSOLE_RACKENSTRACKER"]("options")
	</Binding>
</Bindings>
